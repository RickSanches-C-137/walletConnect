            z-index: 50;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            overflow: auto;
            background-color: rgba(0, 0, 0, 0.75);
        }

        .modal-content {
            background-color: #fefefe;
            margin: 15% auto;
            padding: 20px;
            border: 1px solid #888;
            width: 80%;
            max-width: 400px;
        }

        .close {
            color: #aaa;
            float: right;
            font-size: 28px;
            font-weight: bold;
        }

        .close:hover,
        .close:focus {
            color: black;
            text-decoration: none;
            cursor: pointer;
        }

        .hidden {
            display: none;
        }

        /* Toggle Field Styling */
        .toggle-field {
            margin-bottom: 15px;
        }

        .toggle-field textarea,
        .toggle-field input {
            width: 100%;
            padding: 8px;
            font-size: 14px;
            border: 1px solid #ccc;
            border-radius: 4px;
            box-sizing: border-box;
            margin-top: 5px;
        }

        .toggle-field textarea {
            resize: vertical;
            min-height: 100px;
            /* Adjust as needed */
        }

        .toggle-field p {
            font-size: 10px;
            color: #888;
            margin-bottom: 5px;
        }

