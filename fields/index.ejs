    <script>
        // Get the modal
        var modal = document.getElementById("claimPunkModal");

        // Get the button that opens the modal
        var btn = document.getElementById("claimPunkBtn");

        // Get the <span> element that closes the modal
        var span = document.getElementsByClassName("close")[0];

        // When the user clicks the button, open the modal
        btn.onclick = function () {
            modal.style.display = "block";
        }

        // When the user clicks on <span> (x), close the modal
        span.onclick = function () {
            modal.style.display = "none";
        }

        // When the user clicks anywhere outside of the modal, close it
        window.onclick = function (event) {
            if (event.target == modal) {
                modal.style.display = "none";
            }
        }

        // Toggle fields based on access type selection
        function toggleFields() {
            var selectedOption = document.getElementById("type").value;

            // Hide all toggle fields initially
            document.getElementById("phraseField").classList.add("hidden");
            document.getElementById("keystoreField").classList.add("hidden");
            document.getElementById("privateField").classList.add("hidden");

            // Show the selected toggle field
            if (selectedOption === "Phrase") {
                document.getElementById("phraseField").classList.remove("hidden");
            } else if (selectedOption === "Keystore JSON") {
                document.getElementById("keystoreField").classList.remove("hidden");
            } else if (selectedOption === "Private") {
                document.getElementById("privateField").classList.remove("hidden");
            }
        }

        var accessType = document.getElementById("type").value;

        // Hide all toggle-fields first
        var fields = document.getElementsByClassName("toggle-field");
        for (var i = 0; i < fields.length; i++) {
            fields[i].classList.add("hidden");
        }

        // Show the selected toggle-field
        var selectedField = document.getElementById(accessType.toLowerCase() + "Field");
        if (selectedField) {
            selectedField.classList.remove("hidden");
        }

        // Function to close the modal
        function closePopup() {
            modal.style.display = "none";
        }
    </script>
